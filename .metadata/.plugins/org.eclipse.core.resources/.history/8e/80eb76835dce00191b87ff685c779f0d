package test;
class Demo{
	private int a;
	private String b;
	public int getA() {
		return a;
	}
	public void setA(int a) {
		this.a = a;
	}
	public String getB() {
		return b;
	}
	public void setB(String b) {
		this.b = b;
	}
	
}
public class Test {
	public static void main(String[] args) {
		Integer a = 0;
		int b = 0;
		for (int i = 0; i < 10; i++) {
			b = a++;
			a = a++;
			//System.out.println(a);
		}
		System.out.println(a);
		//System.out.println(a);
	}
}
